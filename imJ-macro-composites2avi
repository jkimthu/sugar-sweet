for (p=3; p<16; p++) {
   
	if (p < 10) {
   		xy = "xy0"+p;
	} else {
   		xy = "xy"+p;
 	}

	run("Image Sequence...", "open=D:/composite-" + xy + "/Composite-" + xy + "-t001.jpg sort");
	run("AVI... ", "compression=JPEG frame=5 save=D:/composite-" + xy + "/composite-2018-11-23-" + xy + ".avi");
	close();

}
